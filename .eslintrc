{
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4],
    "react/self-closing-comp": ["warn",
      {
        "component": true,
        "html": true
      }
    ]
  },
  "parser": "babel-eslint"
}
